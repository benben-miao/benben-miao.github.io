<div class="nexmoe-widget-wrap">
    <h3 class="nexmoe-widget-title"><%= __('social') %></h3>
    <div class="nexmoe-widget nexmoe-social">
        <% for (name in theme.social) { %><a class="mdui-ripple" href="<%- url_for(theme.social[name][0]) -%>" target="_blank" mdui-tooltip="{content: '<%= name %>'}" style="color: <%= theme.social[name][2] %>;background-color: <%= theme.social[name][3] %>;">
            <i class="nexmoefont <% if (theme.social[name][1]){ %><%= theme.social[name][1] %><%} else{ %><%= 'icon-unorderedlist' %><% } %>"></i>
        </a><% } %>
    </div>
    <hr/>
<!-- https://clustrmaps.com -->
<script type='text/javascript' id='clustrmaps' src='//cdn.clustrmaps.com/map_v2.js?cl=ffffff&w=a&t=tt&d=2LlOs1moz-K8B-p9A0x0aIHu3QVuKMC40q9JHNA5XH4&co=0a77c4&cmo=3acc3a&cmn=ff5353&ct=ffffff'></script>
</div>